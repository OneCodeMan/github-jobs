# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'GithubJobs' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for GithubJobs
  pod 'Alamofire', '~> 4.7'
  pod 'IGListKit', '~> 3.0'
  pod 'BEMCheckBox'
  pod 'SwiftSoup'
  pod 'SVProgressHUD'

  target 'GithubJobsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GithubJobsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
